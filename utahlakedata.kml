				<latitude>43.04053300008663</latitude>
				<altitude>0</altitude>
				<heading>7.063693892062323e-011</heading>
				<tilt>0</tilt>
				<range>1000.000012294738</range>
				<gx:altitudeMode>relativeToSeaFloor</gx:altitudeMode>
			</LookAt>
			<styleUrl>#bluedot</styleUrl>
			<!-- styleUrl>#m_ylw-pushpin0</styleUrl -->
